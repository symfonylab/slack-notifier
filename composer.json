{
    "name": "symfonylab/slack-notifier",
    "type": "symfony-bridge",
    "description": "Symfonylab Slack Notifier Bridge",
    "keywords": ["slack", "notifier"],
    "homepage": "https://symfony.com",
    "license": "MIT",
    "require": {
        "php": ">=7.2.5",
        "symfony/http-client": "^4.3|^5.0",
        "symfony/notifier": "~5.0.0"
    },
    "require-dev": {
        "symfony/event-dispatcher": "^4.3|^5.0"
    },
    "autoload": {
        "psr-4": { "Symfony\\Component\\Notifier\\Bridge\\Slack\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "5.0-dev"
        }
    }
}
